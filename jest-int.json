{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".int-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "/argon2(.*)": "<rootDir>/node_modules/argon2$1",
    "src/(.*)": "<rootDir>/src/$1"
  }
}
